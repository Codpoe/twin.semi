{
  "name": "twin.semi",
  "version": "0.1.1",
  "description": "A tailwind color preset for Semi Design",
  "keywords": [
    "tailwind preset",
    "semi design"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codpoe/twin.semi.git"
  },
  "bugs": {
    "url": "https://github.com/Codpoe/twin.semi/issues"
  },
  "homepage": "https://github.com/Codpoe/twin.semi#readme",
  "author": "Codpoe <codpoe.me@gmail.com> (https://github.com/codpoe)",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "dev": "tsc -w",
    "build": "rimraf dist && tsc",
    "gen-variables": "tsx ./scripts/gen-variables.ts",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "check-type": "tsc --noEmit",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "devDependencies": {
    "@codpoe/eslint-config": "^0.1.4",
    "@douyinfe/semi-theme-default": "^2.26.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.35",
    "eslint": "^8.31.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "vitest": "^0.26.2"
  }
}
